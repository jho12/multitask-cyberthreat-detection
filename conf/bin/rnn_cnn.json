{
  "model":{
    "arch":"rnn_cnn",
    "weight_init":"xavier",
    "use_chars":true,
    "c_dim":100,
    "c_in_dropout":0.0,
    "c_rnn_out":"maxpool",
    "c_rnn_layers": [
      {"cell": "lstm","dim": 200,"n_layers": 1,"bidirectional": true}
    ],
    "pretrained":null,
    "w_dim":200,
    "w_in_dropout":0.0,
    "w_cnn_layers": [
      {"height": 2,"filters": 128,"dilation": 1}
    ],
    "mid_dropout":0.0
  },
  "optim":{
    "method":"adam",
    "lr":0.001,
    "momentum":0.0,
    "l2_loss":0.0,
    "lr_scheduler":null,
    "step":25,
    "decay":1.0
  },
  "train":{
    "batch_size":256,
    "max_epochs":200,
    "val_steps":5,
    "patience":5
  }
}
