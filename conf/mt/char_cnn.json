{
  "model": {
    "arch": "char_cnn",
    "weight_init": "xavier",
    "c_dim": 200,
    "c_in_dropout": 0.0,
    "c_cnn_layers": [
      {
        "height": 3,
        "filters": 256,
        "dilation": 1
      }
    ],
    "pretrained": null,
    "w_in_dropout": 0.0,
    "w_bin_out":"maxpool",
    "w_cnn_layers": [
      {
        "height": 2,
        "filters": 128,
        "dilation": 1
      }
    ],
    "mid_dropout": 0.3,
    "use_crf":true
  },
  "optim":{
    "mode":"joint",
    "bin":{
      "weight":0.5,
      "method":"adam",
      "lr":0.001,
      "momentum":0.0,
      "l2_loss":0.0,
      "lr_scheduler":null,
      "step":25,
      "decay":1.0
    },
    "ner":{
      "weight":0.5,
      "method":"adam",
      "lr":0.001,
      "momentum":0.0,
      "l2_loss":0.0,
      "lr_scheduler":null,
      "step":25,
      "decay":1.0
    }
  },
  "train": {
    "batch_size": 256,
    "max_steps": 500,
    "val_steps": 5,
    "patience": 5
  }
}
